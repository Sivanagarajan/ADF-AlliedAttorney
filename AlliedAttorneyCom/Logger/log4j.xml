<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">



  <Appenders>
      <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  
  
  <Loggers>
    <Logger name="com.mst.sutomation.allied.regressiontestcases.DelegateApplication" level="trace" additivity="false">
          <AppenderRef ref="Console"/>
      
    </Logger>
    </Loggers>
  
  <Loggers>
    <Root level="trace">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>

